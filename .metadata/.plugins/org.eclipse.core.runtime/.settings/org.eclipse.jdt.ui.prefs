uBCF5\uBB38 \uB3CC\uBA74\uC11C ResultSet\uC758 Cursor\uB97C \uD55C\uCE78\uC529 \uB0B4\uB9B0\uB2E4.&\#13;\r\n\t\t\twhile(rs.next()) {&\#13;\r\n\t\t\t\t// cursor\uAC00 \uC704\uCE58\uD55C \uACF3\uC758 \uCE7C\uB7FC \uB370\uC774\uD130 \uCD94\uCD9C\uD574\uC11C \uC5B4\uB518\uAC00\uC5D0 \uB2F4\uAE30&\#13;\r\n\t\t\t\t&\#13;\r\n\t\t\t}&\#13;\r\n\t\t} catch (Exception e) {&\#13;\r\n\t\t\t// \uD639\uC2DC \uC608\uC678\uAC00 \uBC1C\uC0DD\uD55C\uB2E4\uBA74 \uC608\uC678 \uC815\uBCF4\uB97C \uCF58\uC194\uC5D0 \uCD9C\uB825\uD574\uC11C \uC5D0\uB7EC\uC758 \uC6D0\uC778 \uCC3E\uAE30&\#13;\r\n\t\t\te.printStackTrace();&\#13;\r\n\t\t} finally {&\#13;\r\n\t\t\ttry {&\#13;\r\n\t\t\t\tif(rs\!\=null) rs.close();&\#13;\r\n\t\t\t\tif(pstmt\!\=null) pstmt.close();&\#13;\r\n\t\t\t\tif(conn\!\=null) conn.close(); // Connection \uBC18\uB0A9&\#13;\r\n\t\t\t} catch (Exception e) {}&\#13;\r\n\t\t}&\#13;\r\n\t}</template><template autoinsert\="true" context\="java" deleted\="false" description\="dbupdate, insert, delete" enabled\="true" name\="dbupdate">public boolean insert(MemberDto dto) {&\#13;\r\n\t\tConnection conn \= null;&\#13;\r\n\t\tPreparedStatement pstmt \= null;&\#13;\r\n\t\tint updateRowCount \= 0;&\#13;\r\n\t\ttry {&\#13;\r\n\t\t\t// Connection \uAC1D\uCCB4\uC758 \uCC38\uC870\uAC12 \uC5BB\uC5B4\uC624\uAE30 (Connection Pool \uC5D0\uC11C \uD558\uB098 \uAC00\uC838\uC624\uAE30)&\#13;\r\n\t\t\tconn \= new DbcpBean().getConn();&\#13;\r\n\t\t\t// \uC2E4\uD589\uD560 sql\uBB38&\#13;\r\n\t\t\tString sql \= "INSERT INTO member"&\#13;\r\n\t\t\t\t\t+ " (num, name, addr)"&\#13;\r\n\t\t\t\t\t+ " VALUES(member_seq.NEXTVAL, ?, ?)";&\#13;\r\n\t\t\t// PrepareStatementm \uAC1D\uCCB4\uC758 \uCC38\uC870\uAC12 \uC5BB\uC5B4\uC624\uAE30&\#13;\r\n\t\t\tpstmt \= conn.prepareStatement(sql);&\#13;\r\n\t\t\t// ? \uC5D0 \uAC12\uC744 \uBC14\uC778\uB529 \uD560\uAC8C \uC788\uC73C\uBA74 \uBC14\uC778\uB529 \uD574\uC900\uB2E4.&\#13;\r\n\t\t\tpstmt.setString(1, dto.getName());&\#13;\r\n\t\t\tpstmt.setString(2, dto.getAddr());&\#13;\r\n\t\t\t// insert, update, delete \uB97C \uC218\uD589\uD558\uACE0 \uBCC0\uD654\uB41C row \uC758 \uAC2F\uC218\uB97C return \uBC1B\uAE30&\#13;\r\n\t\t\tupdateRowCount \= pstmt.executeUpdate();&\#13;\r\n\t\t\tSystem.out.println("\uD68C\uC6D0 \uC815\uBCF4 \uCD94\uAC00 \uC131\uACF5");&\#13;\r\n\t\t\t// \uBC18\uBCF5\uBB38 \uB3CC\uBA74\uC11C ResultSet\uC758 Cursor\uB97C \uD55C\uCE78\uC529 \uB0B4\uB9B0\uB2E4.&\#13;\r\n\t\t} catch (Exception e) {&\#13;\r\n\t\t\t// \uD639\uC2DC \uC608\uC678\uAC00 \uBC1C\uC0DD\uD55C\uB2E4\uBA74 \uC608\uC678 \uC815\uBCF4\uB97C \uCF58\uC194\uC5D0 \uCD9C\uB825\uD574\uC11C \uC5D0\uB7EC\uC758 \uC6D0\uC778 \uCC3E\uAE30&\#13;\r\n\t\t\te.printStackTrace();&\#13;\r\n\t\t} finally {&\#13;\r\n\t\t\ttry {&\#13;\r\n\t\t\t\tif(pstmt\!\=null) pstmt.close();&\#13;\r\n\t\t\t\tif(conn\!\=null) conn.close(); // Connection \uBC18\uB0A9&\#13;\r\n\t\t\t} catch (Exception e) {}&\#13;\r\n\t\t}&\#13;\r\n\t\tif(updateRowCount &gt; 0) {&\#13;\r\n\t\t\treturn true;&\#13;\r\n\t\t} else {&\#13;\r\n\t\t\treturn false;&\#13;\r\n\t\t}&\#13;\r\n\t}</template></templates>
